<?xml version="1.0" encoding="UTF-8"?>
<skin id="bootstrap" name="bootstrap" version="1.0" provider-name="thecrumb" lastModified="2013-04-23">
	<description>NCDevCon Bootstrap Theme for Mango Blog. This theme has customizations specific for the NCDevCon site. For use on your site some modifications may be necessary.</description>
	<url></url>
	<author>Jim Priest</author>
	<authorUrl>http://www.thecrumb.com.com</authorUrl>
	<designAuthor>Jim Priest</designAuthor>
	<designAuthorUrl>http://www.thecrumb.com</designAuthorUrl>
	<thumbnail>img/ncdevcon-bootstrap.jpg</thumbnail>
	<preview>img/ncdevcon-bootstrap.jpg</preview>
	<requiresVersion match="greaterOrEqual">1.7</requiresVersion>
	<adminEditorCss></adminEditorCss>
	<license>Creative Commons Attribution-ShareAlike 2.5</license>
	<pageTemplates>
		<template file="home_index.cfm" name="Home Page" />
		<template file="archives_index.cfm" name="Archives Listing Page" />
	</pageTemplates>
	<adminPageTemplates>
		<template id="login" file="login.cfm" />
	</adminPageTemplates>
	<podLocations>
		<location id="sidebar" name="General sidebar">
			<!-- template pods defined in the skin -->
			<pod id="blog-description">Blog Description</pod>
			<pod id="posts">Recent Entries</pod>
			<pod id="categories">Categories</pod>
			<pod id="search">Search</pod>
			<pod id="category-cloud">Tag Cloud</pod>
			<pod id="monthly-archives">Archives (monthly)</pod>
			<pod id="links-by-category">Links by category</pod>
		</location>
		<location id="sidebar-page" name="Page Sidebar">
			<pod id="page-menu">Page Navigation</pod>
			<pod id="posts">Recent Entries</pod>
			<pod id="categories">Categories</pod>
			<pod id="search">Search</pod>
			<pod id="category-cloud">Tag Cloud</pod>
			<pod id="monthly-archives">Archives (monthly)</pod>
			<pod id="links-by-category">Links by category</pod>
		</location>
		<location id="sidebar-archives" name="Archives Sidebar">
			<pod id="posts">Recent Entries</pod>
			<pod id="categories">Categories</pod>
			<pod id="search">Search</pod>
			<pod id="category-cloud">Tag Cloud</pod>
			<pod id="monthly-archives">Archives (monthly)</pod>
			<pod id="links-by-category">Links by category</pod>
		</location>
	</podLocations>
</skin>